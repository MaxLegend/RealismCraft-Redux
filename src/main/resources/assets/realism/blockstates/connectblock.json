{
    "multipart": [
        {   "when": { "default": "true" },
            "apply": { "model": "realism:connectblock" }
        },
     	{   "when": { "down": "true" },
            "apply": { "model": "realism:connectblockdown", "uvlock": true }
        },
     	{   "when": { "up": "true" },
            "apply": { "model": "realism:connectblockup", "uvlock": true }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "realism:connectblocknorth", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "realism:connectblockeast", "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "realism:connectblocksouth","uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "realism:connectblockwest", "uvlock": true }
        }
    ]
}
